{
  "name": "sharegood-utils",
  "version": "0.0.3",
  "description": "Contains useful JavaScript utilities and GitFlow commands for FE project",
  "bin": {
    "sg-log": "./git-log.sh"
  },
  "main": "index.js",
  "scripts": {
    "server": "node build/app.js",
    "test": "cross-env BABEL_ENV='test' jest --coverage",
    "dev": "better-npm-run dev",
    "build": "better-npm-run build",
    "libs": "npm run build helpers && npm run build http",
    "release": "sh ./build/release.sh"
  },
  "betterScripts": {
    "build": {
      "command": "node build",
      "env": {
        "BUILD": "production"
      }
    },
    "dev": {
      "command": "node build",
      "env": {
        "BUILD": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majinhui04/jinhui-shared-utils.git"
  },
  "keywords": [
    "npm",
    "rollup"
  ],
  "author": "jinhui.m",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/majinhui04/jinhui-shared-utils/issues"
  },
  "homepage": "https://github.com/majinhui04/jinhui-shared-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/preset-env": "^7.6.2",
    "babel-jest": "^23.6.0",
    "babelrc-rollup": "^3.0.0",
    "better-npm-run": "0.0.15",
    "cli-color": "^1.2.0",
    "cross-env": "^6.0.3",
    "cssnano": "^3.10.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-nested": "^2.1.2",
    "postcss-simple-vars": "^4.1.0",
    "rollup": "^0.43.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "inquirer": "^7.0.0",
    "watch": "^1.0.2",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "js-md5": "^0.7.3",
    "moment": "^2.24.0",
    "qs": "^6.9.1"
  }
}
